{{wrap with="admin_modal_page_layout.phtml" into="content_zone"}}

  <h2>$_ENV</h2>

  <table border="1">
    {{list using="{$_ENV}" as="$value" key="$key"}}
      {{list:item}}
        <tr>
          <td>{$key}</td>
          <td><?=(is_array($value))? implode(', ', $value): $value?></td>
        </tr>
      {{/list:item}}
    {{/list}}
  </table>

{{/wrap}}
